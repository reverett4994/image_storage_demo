<p id="notice"><%= notice %></p>
<p>
  <strong>User:</strong>
  <%= @album.user.email %>
</p>
<p>
  <strong>Name:</strong>
  <%= @album.name %>
</p>
<p>
  <strong>Images:</strong>
  <% @album.images.each do |image| %>
    <%= image_tag image.file.url%>
  <%end%>
</p>

<%= link_to 'Edit', edit_album_path(@album) %> |
<%= link_to 'Back', albums_path %>
